{
  "compilerOptions": {
    "target": "ES2020",
    "module": "ESNext",
    "moduleResolution": "Node",
    "lib": [
      "ESNext",
      "ESNext.AsyncIterable",
      "DOM"
    ],
    "esModuleInterop": true,
    "allowJs": true,
    "sourceMap": true,
    "strict": true,
    "experimentalDecorators": true,
    "baseUrl": ".",
    "paths": {
      "~/*": [
        "./*"
      ],
      "@/*": [
        "./*"
      ]
    },
    "types": [
      "@types/node",
      "@nuxt/types",
      "@nuxtjs/axios",
      "@types/jquery",
      "@types/webpack-env",
      "@/types/simplebar",
      "@/types/shims-vue",
      "@/types/vue-events"
    ],
    "suppressImplicitAnyIndexErrors": true,
  },
  "exclude": [
    "node_modules",
    ".nuxt",
    "dist"
  ]
}
